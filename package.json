{
  "name": "kaleidoswap",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "format": "prettier --write \"./src/**/*.{js,ts}\"",
    "lint": "ng lint",
    "prepare": "tsc",
    "postinstall": "patch-package && node patch.js",
    "generate-types": "typechain --target=ethers-v5 'build/contracts/*.json'",
    "fork": "dotenv cross-var ganache-cli --fork %INFURA_URI% --account='%PRIVATE_KEY%,1000000000000000000000'",
    "deploy:local": "deploy.sh development",
    "deploy:fork": "deploy.sh mainnetFork",
    "deploy:testnet": "deploy.sh testnet",
    "deploy:mainnet": "deploy.sh mainnet",
    "deploy:testnet:dry-run": "truffle migrate --reset --network testnet --dry-run",
    "deploy:mainnet:dry-run": "truffle migrate --reset --network mainnet --dry-run",
    "test": "jest --detectOpenHandles",
    "docker:build": "docker build -t ilovelili/kaleidoswap .",
    "docker:build:no-cache": "docker build --no-cache -t ilovelili/kaleidoswap .",
    "docker:run": "docker run -d ilovelili/kaleidoswap"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/localize": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@babel/preset-typescript": "^7.12.1",
    "@coreui/angular": "^3.0.0-alpha.7",
    "@coreui/chartjs": "^2.0.0",
    "@coreui/coreui-pro": "3.2.2",
    "@coreui/icons": "^2.0.0-beta.5",
    "@coreui/icons-angular": "^1.0.0-alpha.1",
    "@coreui/utils": "^1.3.1",
    "@pascalhonegger/ng-datatable": "^9.0.1",
    "@truffle/contract": "^4.0.33",
    "@truffle/hdwallet-provider": "^1.1.1",
    "angular-calendar": "^0.28.16",
    "angular2-ladda": "^2.0.1",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "classlist.js": "^1.1.20150312",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "date-fns": "^1.30.1",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.19",
    "moment": "^2.27.0",
    "mutationobserver-shim": "^0.3.7",
    "ng-select": "^1.0.2",
    "ng2-charts": "~2.3.3",
    "ng2-dragula": "^2.1.1",
    "ngx-bootstrap": "^6.1.0",
    "ngx-perfect-scrollbar": "^9.0.0",
    "ngx-toastr": "^12.1.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "rxjs": "^6.6.0",
    "rxjs-compat": "^6.6.0",
    "spinkit": "^2.0.1",
    "ts-helpers": "^1.1.2",
    "tslib": "^1.13.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.14",
    "babel-jest": "^26.6.1",
    "codelyzer": "^5.2.2",
    "cross-var": "^1.1.0",
    "ganache-cli": "^6.12.1",
    "ganache-core": "^2.13.1",
    "jest": "^26.6.1",
    "prettier": "^2.1.1",
    "truffle-flattener": "^1.5.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typechain": "^3.0.0",
    "typechain-target-ethers-v5": "^1.2.2",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": ">= 10.13",
    "npm": ">= 6"
  }
}
