{
  "name": "kaleidoswap",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "prepare": "tsc",
    "postinstall": "patch-package && node patch.js",
    "generate-types": "typechain --target=ethers-v5 'build/contracts/*.json'",
    "fork": "dotenv cross-var ganache-cli --fork %INFURA_URI% --account='%PRIVATE_KEY%,1000000000000000000000'",
    "deploy:local": "deploy.sh development",
    "deploy:fork": "deploy.sh mainnetFork",
    "deploy:testnet": "deploy.sh testnet",
    "deploy:mainnet": "deploy.sh mainnet",
    "deploy:testnet:dry-run": "truffle migrate --reset --network testnet --dry-run",
    "deploy:mainnet:dry-run": "truffle migrate --reset --network mainnet --dry-run",
    "test": "jest --detectOpenHandles",
    "docker:build": "docker build -t ilovelili/kaleidoswap .",
    "docker:build:no-cache": "docker build --no-cache -t ilovelili/kaleidoswap .",
    "docker:run": "docker run -d ilovelili/kaleidoswap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.1",
    "@angular/cdk": "^10.2.6",
    "@angular/common": "10.2.1",
    "@angular/compiler": "10.2.1",
    "@angular/core": "10.2.1",
    "@angular/forms": "10.2.1",
    "@angular/material": "^10.2.6",
    "@angular/platform-browser": "10.2.1",
    "@angular/platform-browser-dynamic": "10.2.1",
    "@angular/platform-server": "10.2.1",
    "@angular/router": "10.2.1",
    "@babel/preset-typescript": "^7.12.1",
    "@openzeppelin/contracts": "^3.2.0",
    "@truffle/contract": "^4.0.33",
    "@truffle/hdwallet-provider": "^1.1.1",
    "core-js": "2.5.7",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.19",
    "node-sass": "4.12.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "rxjs": "^6.5.3",
    "truffle-plugin-verify": "^0.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/core": "10.2.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "10.2.1",
    "@angular/language-service": "10.2.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "babel-jest": "^26.6.1",
    "codelyzer": "^5.1.2",
    "cross-var": "^1.1.0",
    "ganache-cli": "^6.12.1",
    "ganache-core": "^2.13.1",
    "jest": "^26.6.1",
    "truffle-flattener": "^1.5.0",
    "ts-node": "^8.4.1",
    "tslint": "~6.1.0",
    "typechain": "^3.0.0",
    "typescript": "4.0.5",
    "webpack": "^4.40.2",
    "webpack-dev-server": "^3.8.1"
  }
}